  "description": "Totonga arts api version 2",
  "main": "build/src/app.js",
  "scripts": {
    "test": "mocha 'src/tests/**/*.js' --recursive --reporter mocha-junit-reporter --timeout 60000 --exit",
    "start:dev": "npx nodemon",
    "start:prod": "npm run build && node build/src/app.js",
    "build": "rimraf ./build && tsc",
    "start": "node build/src/app.js",
    "start-pm2:dev": "pm2 restart ecosystem.config.js --only local",
    "start-pm2:staging": "pm2 restart ecosystem.config.js --only staging",
    "start-pm2:prod": "pm2 restart ecosystem.config.js --only production"
  },
